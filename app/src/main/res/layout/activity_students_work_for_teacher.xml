<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.myprozhepayanterm.StudentPage">

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar_teachermanagment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/toolbar"
        android:theme="@style/ThemeOverlay.AppCompat.Dark"
        tools:ignore="MissingConstraints" />

    <FrameLayout
        android:id="@+id/fragment_container_teachermanagment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="sss"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <android.support.design.widget.BottomNavigationView
        android:id="@+id/navigation_teachermanagment"
        android:layout_width="411dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/toolbar_teachermanagment"
        android:layout_marginTop="0dp"
        android:background="@color/toolbar"
        app:itemIconTint="#ffffff"
        app:itemTextColor="#ffffff"
        app:layout_constraintTop_toBottomOf="@+id/toolbar_teachermanagment"
        app:menu="@menu/menu_for_studentsforteacher_nav"
        tools:ignore="MissingConstraints,UnknownId"
        tools:layout_editor_absoluteX="0dp" />
</RelativeLayout>
